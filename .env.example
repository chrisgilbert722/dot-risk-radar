# Supabase Configuration (Required)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# FMCSA API Configuration (Required for carrier data)
FMCSA_WEBKEY=your-fmcsa-webkey-here

# Stripe Configuration (Required for paid monitoring subscriptions)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# Feature Flags (optional - defaults to false)
FEATURE_COMPLIANCE_VAULT=false
FEATURE_ALERT_DELIVERY=false
FEATURE_DATAQS_ASSIST=false
FEATURE_FLEET_ROLLUPS=false
FEATURE_PROGRAMMATIC_SEO=false
